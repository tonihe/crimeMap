        mapbox:mapbox_cameraTargetLat="49.2193226"
        mapbox:mapbox_cameraTargetLng="-122.598398"
        mapbox:mapbox_styleUrl="mapbox://styles/serotoninplus/cjotd814rcrwq2spzsgxu80yg"
